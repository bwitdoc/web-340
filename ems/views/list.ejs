<div role="main" class="container" style="width: 50%">
    <br/> <br/>
        <% if (employees.length) { %>
    
    <h2> --Employee Listing-- </h2><br/>
    <a href="/new">Add New</a>
    <tableclass="table table-hover">
    <thead class="thead-dark">
        <tr>
            <th>Name</th>
            <th>Functions</th>
        </tr>
    </thead>
    <tbody>
        <% employee.forEach(function (entry) { %>
            <tr>
                <td><%= entry.name %></td>
                <td><a href="/view/<%= entry.name %>">VIEW</a></td>
            </tr>
        </tbody>
       <% }) %>
    </table>
    <% } else { %>
    No entries! <a href="/new">Add one!</a>
<% } %>
<br/>
    </div>
    